        <groupId>cn.koala</groupId>
        <artifactId>koala-mybatis-starter</artifactId>
        <version>${revision}</version>
      </dependency>
      <dependency>
        <groupId>cn.koala</groupId>
        <artifactId>koala-security-starter</artifactId>
        <version>${revision}</version>
      </dependency>
      <dependency>
        <groupId>cn.koala</groupId>
        <artifactId>koala-system-starter</artifactId>
        <version>${revision}</version>
      </dependency>
      <dependency>
        <groupId>cn.koala</groupId>
        <artifactId>koala-web-starter</artifactId>
        <version>${revision}</version>
      </dependency>
    </dependencies>
