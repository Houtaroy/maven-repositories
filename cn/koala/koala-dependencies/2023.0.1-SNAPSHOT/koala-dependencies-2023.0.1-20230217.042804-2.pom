  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>cn.koala</groupId>
    <artifactId>koala</artifactId>
    <version>2023.0.1-SNAPSHOT</version>
  </parent>
  <groupId>cn.koala</groupId>
  <artifactId>koala-dependencies</artifactId>
  <version>2023.0.1-SNAPSHOT</version>
  <packaging>pom</packaging>
